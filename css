    padding: .75rem;
    vertical-align: top;
    border-top: 1px solid #ddd
}

.table thead th {
    vertical-align: bottom;
    border-bottom: 2px solid #ddd
}

.table tbody+tbody {
    border-top: 2px solid #ddd
}

.table-sm td,.table-sm th {
    padding: .3rem
}

.table-bordered {
    border: 1px solid #ddd
}

.table-bordered td,.table-bordered th {
    border: 1px solid #ddd
}

